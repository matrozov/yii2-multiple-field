{
    "name": "matrozov/yii2-multiple-field",
    "description": "Multiple field for Yii2",
    "keywords": [
        "yii2",
        "yii2-extension",
        "yii2 multiple input",
        "yii2 multiple field",
        "yii2 array input",
        "yii2 array validate"
    ],
    "homepage": "https://github.com/matrozov/yii2-multiple-field",
    "type": "extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Matrozov",
            "email": "oleg.matrozov@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/matrozov/yii2-multiple-field/issues",
        "source": "https://github.com/matrozov/yii2-multiple-field"
    },
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.14"
    },
    "autoload": {
        "psr-4": {
            "matrozov\\yii2multipleField\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
